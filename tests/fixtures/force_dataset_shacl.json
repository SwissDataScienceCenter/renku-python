{
    "@context": {
       "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
       "sh": "http://www.w3.org/ns/shacl#",
       "schema": "http://schema.org/"
    },
    "@graph": [
        {
            "@id": "_:forceDatasetShape",
            "@type": "sh:NodeShape",
            "sh:targetNode": "schema:Dataset",
            "sh:property": [
                {
                    "sh:path": [
                        {
                            "sh:inversePath": {
                                "@id": "rdf:type"
                             }
                        }
                    ],
                    "sh:minCount": 1
                }
            ]
        }
    ]
}
